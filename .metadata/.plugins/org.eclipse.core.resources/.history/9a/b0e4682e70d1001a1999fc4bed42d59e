package leetcodePratice;

import java.util.HashMap;
import java.util.Map.Entry;

public class exercise19_LeetCode136_Single_Number {

	/*
	 * Given a non-empty array of integers, every element appears twice except for
	 * one. Find that single one.
	 * 
	 * Note: Your algorithm should have a linear runtime complexity. Could you
	 * implement it without using extra memory?
	 * 
	 * Example 1: Input: [2,2,1] Output: 1
	 * 
	 * Example 2: Input: [4,1,2,1,2] Output: 4
	 */

	// 方法一 直觀用map存起來
	static int singleNumberFunction1(int[] nums) {
		int singleNumber = -1;
		HashMap<Integer, Integer> map = new HashMap<Integer, Integer>();
		
		for (int i = 0; i < nums.length; i++) {
			
			if (map.containsKey(nums[i])) {
					int count = map.get(nums[i])+1;
					map.put(nums[i], count);				
			} else {
				map.put(nums[i], 1);
			}
		}	
		for(Entry<Integer, Integer> s:map.entrySet()) {
			if(s.getValue()==1) {
				singleNumber=s.getKey();
				break;
			}	
		}
		return singleNumber;
	}
	
	
	/*
	 * 上網看到的非人類解法...
	 */
	static int singleNumberFunction2(int[] nums) {
		int singleNumber = -1;
		
		
		
		return singleNumber;
	}
	
	

	public static void main(String[] args) {
		//int[] nums = { 2, 2, 1 };
		int[] nums = { 4,1,2,1,2 };
		
		int ans = singleNumberFunction1(nums);
		System.out.println(ans);
		
		int ans = singleNumberFunction2(nums);
		System.out.println(ans);

	}

}
