package leetcodePratice;

public class test {

	public static void fizzBuzz(int n) {

		for (int i = 1; i <= n; i++) {
			if (i % 15 == 0) {
				System.out.println("FizzBuzz");
			}

			if (i % 15 != 0 && i % 3 == 0) {
				System.out.println("Fizz");

			} else if (i % 15 != 0 && i % 5 == 0) {
				System.out.println("Buzz");
			} 
		}

	}

	public static void main(String[] args) {

		fizzBuzz(16);

	}

}
